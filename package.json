{
  "name": "style-dictionary-example-complete",
  "version": "1.0.0",
  "description": "",
  "main": "build.js",
  "scripts": {
    "build-transform-default-light": "token-transformer input.json tokens/default-light.json Ref,sys-default-light,usage-colors,usage-type,sys-type-medium_gt1024",
    "build-transform-default-dark": "token-transformer input.json tokens/default-dark.json Ref,sys-default-dark,usage-colors,usage-type,sys-type-medium_gt1024",
    "build-transform-type-lg": "token-transformer input.json tokens/sys-type-large_gt1920.json sys-type-large_gt1920",
    "build-transform-type-md": "token-transformer input.json tokens/sys-type-medium_gt1024.json sys-type-medium_gt1024",
    "build-transform-type-sm": "token-transformer input.json tokens/sys-type-small.json sys-type-small",
    "build-transform": "npm run build-transform-default-light && npm run build-transform-default-dark",
    "build": "style-dictionary build",
    "build-sd": "node build.js",
    "build-tw": "postcss ./styles.css -o ./tailwind.css",
    "clean": "rm -rf build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "style-dictionary": "^3.7.2",
    "token-transformer": "^0.0.29",
    "autoprefixer": "^10.4.13",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.2.4"
  }
}
